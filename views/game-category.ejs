<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Game Category</title>
  <link rel="stylesheet" href="/styles/game-category.css">
</head>
<body>
  <%- include('../public/partials/navbar.ejs') %>

  <div class="container" style="margin-top: 50px;">
    <h1>Game Category</h1>
    <ul class="game-list">
      <% games.forEach(game => { %>
        <li><%= game.game_name %></li>
      <% }); %>
    </ul>
  </div>
</body>
</html>
